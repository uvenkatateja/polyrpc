{
  "name": "@polyrpc/client",
  "version": "0.2.0",
  "description": "Type-safe Python API client for TypeScript",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "vitest run",
    "lint": "eslint src/",
    "prepublishOnly": "pnpm build"
  },
  "keywords": [
    "polyrpc",
    "python",
    "typescript",
    "fastapi",
    "pydantic",
    "type-safe",
    "rpc",
    "api",
    "client"
  ],
  "author": "PolyRPC Contributors",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/uvenkatateja/polyrpc.git",
    "directory": "packages/client"
  },
  "bugs": {
    "url": "https://github.com/uvenkatateja/polyrpc/issues"
  },
  "homepage": "https://polyrpc.vercel.app",
  "devDependencies": {
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.2.0"
  },
  "peerDependencies": {
    "typescript": ">=4.7.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
